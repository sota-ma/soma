{
  "name": "sota-of-medicalai",
  "version": "1.0.0",
  "description": "最新の医療AI論文を日本語で紹介します",
  "author": "kkiriyama",
  "private": true,
  "scripts": {
    "dev": "firebase use staging && cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "dev-debug": "node --inspect-brk=9229 node_modules/nuxt/bin/nuxt",
    "build": "nuxt build",
    "setup": "rm -rf public/* && cp -R functions/nuxt/dist/client public/assets",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "lint": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "test": "jest --config jest.config.js --coverage --setupFiles dotenv/config",
    "deploy-production": "firebase deploy --only hosting:production",
    "deploy-preview": "firebase deploy --only hosting:preview"
  },
  "dependencies": {
    "@contentful/rich-text-html-renderer": "^13.1.0",
    "@nuxtjs/axios": "^5.3.6",
    "ajv": "^6.10.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-stage-0": "^6.24.1",
    "@nuxtjs/dotenv": "^1.3.0",
    "bootstrap": "^4.1.3",
    "bootstrap-vue": "^2.0.0-rc.11",
    "contentful": "^7.5.0",
    "cross-env": "^5.2.0",
    "express": "^4.16.4",
    "firebase": "^5.11.0",
    "firebase-admin": "^7.3.0",
    "firebase-functions": "^2.3.0",
    "nuxt": "^2.4.0"
  },
  "devDependencies": {
    "@babel/polyfill": "^7.4.4",
    "@babel/register": "^7.4.4",
    "@nuxtjs/eslint-config": "^0.0.1",
    "@vue/test-utils": "^1.0.0-beta.27",
    "ava": "^1.4.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^5.15.1",
    "eslint-config-standard": ">=12.0.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": ">=2.16.0",
    "eslint-plugin-jest": ">=22.3.0",
    "eslint-plugin-node": ">=8.0.1",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-promise": ">=4.0.1",
    "eslint-plugin-standard": ">=4.0.0",
    "eslint-plugin-vue": "^5.2.2",
    "jest": "^24.1.0",
    "jsdom": "^15.0.0",
    "nodemon": "^1.18.9",
    "vue-jest": "^3.0.3"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  }
}
